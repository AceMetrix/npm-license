{
    "name": "npm-license",
    "description": "Check license info for a module",
    "author": "Duncan Wong <baduncaduncan@gmail.com>",
    "version": "0.1.7-1",
    "dependencies": {
        "mkdirp": "*",
        "treeify": "*",
        "nopt": "*",
        "read-installed": "*",
        "underscore": "~1.4.4"
    },
    "devDependencies": {
        "yui-lint": "~0.1.1",
        "jshint": "~1.1.0",
        "vows": "*"
    },
    "keywords": [
        "license",
        "cli",
        "checker",
        "oss"
    ],
    "main": "./lib/index.js",
    "bin": {
        "npm-license": "./bin/npm-license"
    },
    "scripts": {
        "pretest": "jshint --config ./node_modules/yui-lint/jshint.json ./lib/",
        "test": "istanbul cover --print both vows -- --spec ./tests/*.js"
    },
    "preferGlobal": "true",
    "bugs": {
        "url": "http://github.com/angleman/npm-license/issues"
    },
    "licenses": [{
        "type": "BSD",
        "url": "https://github.com/angleman/npm-license/blob/master/LICENSE"
    }],
    "repository": {
        "type": "git",
        "url": "http://github.com/angleman/npm-license.git"
    }
}
